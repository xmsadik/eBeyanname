    "SOURCE":"@AccessControl.authorizationCheck: #NOT_REQUIRED\r\n@EndUserText.label: 'Online Pos Başlık Verileri'\r\n@Metadata.ignorePropagatedAnnotations: true\r\ndefine root view entity YOP_DDL_I_POSHEADER\r\n  as select distinct from YOP_DDL_I_POSDATA as PosHeader\r\n//  composition [1..1] of YOP_DDL_I_POSTOTAL as _postotal\r\n{ \r\n  key Bukrs,\r\n  key BankNo,\r\n  key BankName,\r\n  key WorkplaceNo,\r\n  key TransactionDate,\r\n  key ValueDate,\r\n  key ProcessType,\r\n  key CurrencyCode,\r\n  WorkplaceDesc,\r\n  AccDocument,\r\n  OrgRefDoc,\r\n  RevDoc,\r\n  gjahr,\r\n      @Semantics.amount.currencyCode : 'CurrencyCode'\r\n      sum( GrossAmount )      as GrossAmount,\r\n      @Semantics.amount.currencyCode : 'CurrencyCode'\r\n      sum( NetAmount )        as NetAmount,\r\n      @Semantics.amount.currencyCode : 'CurrencyCode'\r\n      sum( CommissionAmount ) as CommissionAmount,\r\n      @Semantics.amount.currencyCode : 'CurrencyCode'\r\n      sum( InterruptAmount )  as InterruptAmount,\r\n\r\n      cast(\r\n        case\r\n          when cast( sum( GrossAmount ) as abap.dec( 23, 2 ) ) = 0\r\n          then 0\r\n          else ( cast( sum( CommissionAmount ) as abap.dec( 23, 2 ) )\r\n                 / cast( sum( GrossAmount ) as abap.dec( 23, 2 ) ) )\r\n               * cast( 100 as abap.dec( 23, 2 ) )\r\n        end\r\n        as abap.dec( 23, 2 )\r\n      )                       as CommissionRate\r\n\r\n//      _postotal\r\n}\r\ngroup by\r\n  Bukrs,\r\n  BankNo,\r\n  BankName,\r\n  WorkplaceNo,\r\n  TransactionDate,\r\n  ValueDate,\r\n  ProcessType,\r\n  WorkplaceDesc,\r\n  AccDocument,\r\n  OrgRefDoc,\r\n  RevDoc,\r\n  gjahr,\r\n  CurrencyCode\r\n/*+[internal] {\n\"BASEINFO\":\n{\n\"FROM\":\n[\n\"YOP_DDL_I_POSDATA\"\n],\n\"ASSOCIATED\":\n[],\n\"BASE\":\n[],\n\"ANNO_REF\":\n[],\n\"SCALAR_FUNCTION\":\n[],\n\"VERSION\":0,\n\"ANNOREF_EVALUATION_ERROR\":\"\"\n}\n}*/",
